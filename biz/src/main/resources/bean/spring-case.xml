<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<!-- 社区 公共停车场 -->
	<bean id="caseAction" class="com.papa2.client.cases.action.CaseAction" parent="baseAction" scope="request">
		<property name="caseService" ref="caseService" />
	</bean>
	
	<bean id="caseService" class="com.papa2.client.cases.service.impl.CaseServiceImpl">
		<property name="caseDao" ref="caseDao" />
	</bean>
	
	<bean id="caseDao" class="com.papa2.client.cases.dao.impl.CaseDaoImpl" parent="baseDao" />

</beans>
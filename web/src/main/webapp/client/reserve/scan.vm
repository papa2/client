<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
		<title>扫一扫</title>
		#ICON()
        #CSSM(["bootstrap"])
		<style>
		body { padding-top: 70px; padding-bottom: 70px; background-color: #f5f5f5; }
		</style>
		#UA()
	</head>
	<body>
		<nav class="navbar navbar-default navbar-fixed-top" role="navigation" id="foo">
			<a class="navbar-brand glyphicon glyphicon-chevron-left" href="javascript:back();"></a>
			<div class="navbar-brand pull-right">扫一扫</div>
        </nav>
		
		<div>
			#parse("/client/framework/resultMessage.vm")
			<div class="modal-body">
				<a class="btn btn-success btn-block" style="font-size: 18px;" id="btn" data-loading-text="正在处理..." href="javascript:scanQRCode();">扫一扫</a>
			</div>
        </div>

		#parse("/client/framework/footer.vm")

		#JSM(["jquery", "spin", "weixin"])
		#JS_ENV_VAL()
        <script>
          wx.config({
              debug: false,
              appId: '$!{appId}',
              timestamp: $!{timestamp},
              nonceStr: '$!{nonceStr}',
              signature: '$!{signature}',
              jsApiList: [
                'scanQRCode'
              ]
          });
        </script>
		<script type="text/javascript" src="$!{env.imgUrl}/js/reserve/scan.js?t=20150908"></script>
	</body>
</html>

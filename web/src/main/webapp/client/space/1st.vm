<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
		<title>出租车位</title>
		#ICON()
        #CSSM(["messenger", "bootstrap"])
		<style>
		body { padding-top: 70px; padding-bottom: 70px; }
		</style>
		#UA()
	</head>
	<body>
		<nav class="navbar navbar-default navbar-fixed-top" role="navigation" id="foo">
			<a class="navbar-brand glyphicon glyphicon-chevron-left" href="javascript:goBack();"></a>
			<div class="navbar-brand pull-right">车位信息</div>
        </nav>
		
		<div>
			<div class="modal-header">
				<h4 class="modal-title">1. 请选择地区</h4>
            </div>
			<div class="modal-body">
				<form class="form-horizontal" role="form" method="post">
                    <input type="hidden" name="space.costType" value="$!{space.costType}"></input>
					<input type="hidden" id="bc" name="backCode" value="$!{backCode}"></input>
					<div class="form-group">
						<div class="col-xs-3" style="padding-right: 1px;"><select class="form-control small" id="province" ></select></div>
						<div class="col-xs-3" style="padding-right: 1px; padding-left: 1px;"><select class="form-control small" id="city" ></select></div>
						<div class="col-xs-5" style="padding-left: 1px;"><select class="form-control small" id="area" ></select></div>
                    </div>
                </form>
            </div>
			<div class="modal-footer">
				<a class="btn btn-default btn-lg" id="btn1" data-loading-text="正在处理..." href="javascript:goBack();">上一步</a>
				<a class="btn btn-success btn-lg" id="btn2" data-loading-text="正在处理..." href="javascript:next();">下一步</a>
			</div>
        </div>
		
		#parse("/client/framework/footer.vm")
		
		#JSM(["jquery", "jquery.citytools", "bootstrap", "spin"])
		#JS_ENV_VAL()
		<script type="text/javascript">
		var backCode = '$!{backCode}' == '' ? '3301' : '$!{backCode}';
        </script>
		<script type="text/javascript" src="$!{env.imgUrl}/js/space/1st.js"></script>
    </body>
</html>
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
		<title>出租车位</title>
		#ICON()
        #CSSM(["font-awesome", "bootstrap"])
		<style>
		body { padding-top: 70px; padding-bottom: 70px; }
		</style>
		#UA()
	</head>
	<body>
		<nav class="navbar navbar-default navbar-fixed-top" role="navigation" id="foo">
			<a class="navbar-brand glyphicon glyphicon-chevron-left" href="javascript:home();"></a>
			<div class="navbar-brand pull-right">车位信息</div>
        </nav>
		
		<ul class="list-group">
			#foreach($space in $spaceList)
			<a href="javascript:pay('$!{sapce.spaceId}');" class="list-group-item list-group-item-success" style="margin: 10px 0 10px 0; ">
				<span class="fa fa-chevron-right pull-right" style="margin: 3px 0 0 15px;"></span>
                <i class="fa fa-car" style="margin-right: 5px;"></i><strong> $!{space.spaceCode}</strong>
			</a>
			#end
        </ul>
		
		<div class="container">
			<div class="modal-body">
				<a class="btn btn-success btn-lg btn-block" id="btn" data-loading-text="正在处理..." href="javascript:createSpace();">
					<i class="fa fa-plus"></i> 我要出租车位
				</a>
            </div>
        </div>
		
		#parse("/client/framework/footer.vm")
		
		#JSM(["jquery", "bootstrap", "spin"])
		#JS_ENV_VAL()
		<script type="text/javascript">
        </script>
		<script type="text/javascript" src="$!{env.imgUrl}/js/space/index.js?t=20150818"></script>
    </body>
</html>